<section
  data-section-id="desplegable"
  id="desplegable"
  class="desplegable"
>
  {% for block in section.blocks %}
    {% case block.type %}
      {% when 'text' %}
          <div data-section="" id="block-{{ block.id }}" class="ask">
            {% unless block.settings.title == blank %}
            <div class="title-seccion">{{ block.settings.title }}</div>
            {% endunless %}
          </div>
    {% endcase %}
  {% endfor %}
</section>
{% schema %}
{
  "name": "INFO-BLOCK",
  "blocks": [
    {
      "type": "text",
      "name": "Title",
      "settings": [
        {
          "type": "richtext",
          "id": "title",
          "label": "Title"
        }
      ]
    }
  ],
  "presets": [
  {
    "name": "INFO-BLOCK"
  }
]
}
{% endschema %}
